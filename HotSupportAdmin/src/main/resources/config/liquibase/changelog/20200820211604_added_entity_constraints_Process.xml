<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Process.
    -->
    <changeSet id="20200820211604-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="application_id"
                                 baseTableName="process"
                                 constraintName="fk_process_application_id"
                                 referencedColumnNames="id"
                                 referencedTableName="application"/>

        <addForeignKeyConstraint baseColumnNames="server_id"
                                 baseTableName="process"
                                 constraintName="fk_process_server_id"
                                 referencedColumnNames="id"
                                 referencedTableName="server"/>

    </changeSet>
</databaseChangeLog>
